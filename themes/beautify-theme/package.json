{
  "name": "beautiful-theme",
  "version": "0.0.1-alpha",
  "description": "",
  "private": true,
  "main": "dist/beautify-theme/beautify-theme.umd.js",
  "admin:main": {
    "moduleName": "index",
    "entry": "dist/admin/index.umd.js"
  },
  "widgets": [
    {}
  ],
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "serve": "po dev src/beautify-theme.ts",
    "build:front": "po build --name beautifyTheme --filename index --dest dist/beautify-theme src/beautify-theme.ts",
    "build:widgets": "po build --dest dist/widgets src/widgets",
    "build:admin": "po build --name beautifyThemeAdmin --filename index --dest dist/admin  src/admin/index.ts",
    "build": "yarn build:front && yarn build:widgets && yarn build:admin",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@plumemo/devtools": "^0.0.1-alpha",
    "@vue-async/utils": "^1.0.3",
    "core-js": "^3.6.4",
    "marked": "^1.1.1",
    "moment": "^2.28.0",
    "vue": "^2.6.9",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.1",
    "vuetify": "^2.3.10"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.5",
    "@types/marked": "^1.1.0",
    "@vue-async/module-loader-typing": "^0.0.2",
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-typescript": "^4.5.6",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^7.3.0",
    "typescript": "^3.7.2",
    "vue-router": "^3.4.3",
    "vue-template-compiler": "^2.6.9",
    "vue-tsx-support": "^2.3.3"
  }
}
